{
  "name": "cpdirplus",
  "version": "0.0.5",
  "description": "copy folder to another folder",
  "main": "dist/index.js",
  "bin": {
    "cpdirplus": "bin/cpdirplus"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "rimraf dist && tsc -w",
    "build": "rimraf dist && tsc -b",
    "test": "npm run build && jest",
    "publish:patch": "npm version patch && git push --follow-tags && npm publish",
    "publish:minor": "npm version minor && git push --follow-tags && npm publish",
    "publish:major": "npm version major && git push --follow-tags && npm publish",
    "prepare": "husky"
  },
  "lint-staged": {
    "./src/**/*": "eslint --fix"
  },
  "keywords": [
    "cp-folder",
    "copy-folder",
    "copy-dir",
    "cp-dir",
    "copy-directory",
    "copy",
    "folder",
    "directory",
    "cpdir",
    "cpdirplus"
  ],
  "author": "coder_guanxy",
  "email": "coder_guan@163.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coder-guanxy/cpdirplus.git"
  },
  "bugs": {
    "url": "https://github.com/coder-guanxy/cpdirplus/issues"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.2.0",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@rspack/lite-tapable": "1.0.0-alpha.1",
    "colorette": "^2.0.20",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "yargs": "^17.7.2"
  }
}
